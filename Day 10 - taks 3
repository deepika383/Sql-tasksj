CREATE TABLE Products (
    ProductID int PRIMARY KEY,
    ProductName varchar(50),
    Price int(10)
);

INSERT INTO Products (ProductID, ProductName, Price)
VALUES
(1, 'Laptop', 1300),
(2, 'Phone', 800),
(3, 'computer', 600),
(4, 'Keyboard', 5000),
(5, 'Mouse', 500);

SELECT 
    MAX(Price) AS MaxPrice,
    MIN(Price) AS MinPrice
FROM Products;
